    background-color: #1EBCB7;
    border-color: #1AADA8;
}

.btn-buscar:hover,
.btn-buscar:focus,
.btn-buscar:active,
.btn-buscar.active,
.open .dropdown-toggle.btn-buscar {
    color: #FFFFFF;
    background-color: #0DA6A1;
    border-color: #1AADA8;
}

.btn-buscar:active,
