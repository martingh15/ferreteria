}

.bounce {
    -webkit-animation: bounce 2s;
    -moz-animation: bounce 2s;
    -o-animation: bounce 2s;
    -ms-animation: bounce 2s;
    animation: bounce 2s;
}

@-webkit-keyframes bounce {
    0%,
    20%,
    50%,
    80%,
    100% {
        transform: translateY(0);
    }
    40% {
        transform: translateY(-30px);
    }
    60% {
        transform: translateY(-15px);
    }
}

