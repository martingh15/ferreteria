/*==============Estilo de prueba navLeftRes==================*/

.wrapper {
    padding-left: 0;
    padding-right: 0;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

.wrapper.left-toggled {
    margin-left: 250px;
    width: 100%;
    overflow-x: hidden;
}

.wrapper.right-toggled {
    margin-left: -250px;
    width: 100%;
    overflow-x: hidden;
}

#left-sidebar-wrapper {
    z-index: 1;
    position: fixed;
    left: 250px;
    width: 0;
    top: 0;
    height: 100%;
    margin-left: -250px;
    overflow-y: auto;
    background: #3d3c3c;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#right-sidebar-wrapper {
    z-index: 5;
    position: fixed;
    right: 250px;
    width: 0;
    top: 0;
    height: 100%;
    margin-right: -250px;
    overflow-y: auto;
    background: #484747;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

.wrapper.left-toggled #left-sidebar-wrapper {
    width: 250px;
}

.wrapper.right-toggled #right-sidebar-wrapper {
    width: 250px;
}

