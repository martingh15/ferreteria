@keyframes aparecer {
    0% {
        -webkit-transform: scaleX(0);
    }
    100% {
        -webkit-transform: scaleX(100%);
    }
}

@-webkit-keyframes aparecer {
    0% {
        -webkit-transform: scaleX(0);
    }
    100% {
        -webkit-transform: scaleX(100%);
    }
}

@-moz-keyframes aparecer {
    0% {
        -webkit-transform: scaleX(0);
    }
    100% {
        -webkit-transform: scaleX(100%);
    }
}

@-ms-keyframes aparecer {
    0% {
        -webkit-transform: scaleX(0);
    }
    100% {
        -webkit-transform: scaleX(100%);
    }
}

@-o-keyframes aparecer {
    0% {
        -webkit-transform: scaleX(0);
    }
    100% {
        -webkit-transform: scaleX(100%);
    }
}

