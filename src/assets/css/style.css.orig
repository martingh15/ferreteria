fieldset[disabled] .btn-buscar,
.btn-buscar.disabled:hover,
.btn-buscar[disabled]:hover,
fieldset[disabled] .btn-buscar:hover,
.btn-buscar.disabled:focus,
.btn-buscar[disabled]:focus,
fieldset[disabled] .btn-buscar:focus,
.btn-buscar.disabled:active,
.btn-buscar[disabled]:active,
